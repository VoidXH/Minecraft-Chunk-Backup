<Window x:Class="MinecraftChunkBackup.Restore"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MinecraftChunkBackup"
        mc:Ignorable="d"
        Title="Restore" Height="100" Width="400">
    <Grid>
        <Label Content="Version:" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="version" Margin="67,10,90,0" VerticalAlignment="Top"/>
        <Button Content="OK" Margin="0,10,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="OK"/>
        <Button Content="Cancel" Margin="0,35,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
